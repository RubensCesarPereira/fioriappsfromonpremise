<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	id="idFragOC">
	<SelectDialog
		title="Org. Compra"
		class="sapUiPopupWithPadding"
		items="{ocModel>/results}"
		search="_handleValueHelpSearchOC"
		confirm="_handleValueHelpCloseOC"
		cancel="_handleValueHelpCloseOC">
		<StandardListItem			
			iconDensityAware="false"
			iconInset="false"
			title="{ocModel>Ekorg}"
			description="{ocModel>Ekotx}" />
	</SelectDialog>
</core:FragmentDefinition>